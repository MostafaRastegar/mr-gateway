extends layout

block content
  h1= title
  - var usersArr = result
  table.table.table-striped.mt-5.mb-5
    thead
      tr
        td Id
        td UserName
        td UserPassword
        td TerminalId
    tbody
      each user in usersArr
        tr
          td #{user._id}
          td #{user.userName}
          td #{user.userPassword}
          td #{user.terminalId}
